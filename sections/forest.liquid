{{ "forestMap.css" | asset_url | stylesheet_tag }}
{{ "forestGoogleMap.js" | asset_url | script_tag }}

<div>
  <h1>{{ section.settings.headline }}</h1>
<div id="map"></div>
<script 
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCvbBZMdQ710YM1aSpZf8GGAQpBEnhMOi8&callback=initMap"
    defer
></script>

{% schema %}
  {
    "name": "Forest",
    "settings": [
      {
        "type": "text",
        "label": "Headline",
        "id" : "headline"
      }
    ],
    
    "presets": [{
        "name": "Forest"
      }
    ]
  }
{% endschema %}